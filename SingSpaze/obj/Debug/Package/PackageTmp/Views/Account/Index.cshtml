@model IEnumerable<SingSpaze.Models.userdata>

@{
    ViewBag.Title = "User Lists";
}
<div style="padding-top: 25px;padding-bottom: 30px;">
<table style="width:100%;border: 1px solid black;text-align: center;">
    <tr style="border: 1px solid black;background-color: rgb(218, 216, 216);">
        <td style="text-align: left;padding-left: 15px;"><h2>User Lists</h2></td>
        <td style="text-align: right;" ><input style="margin-top: 30px;" type="button" value="Create New" onclick="location.href='@Url.Action("create", "account")'" /></td>
    </tr>
    <tr>
        <td colspan="2" style="padding: 15px;">
        <table style="border-bottom:1px solid black;">    
    <tr>
        @*<th style="border: 1px solid black;text-align: center;">
            @Html.DisplayNameFor(model => model.originName)
        </th>*@
        <th style="border: 1px solid black;text-align: center;">
            Name
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Email
        </th>
        <th style="border: 1px solid black;text-align: center;">
            FacebookId
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Edit
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Delete
        </th>
        
    </tr>

@foreach (var item in Model)
{
    <tr style="text-align: left;">
        @*<td>
            @Html.DisplayFor(modelItem => item.originName)
        </td>*@
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.Firstname) @Html.DisplayFor(modelItem => item.Lastname)  
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.fbUserId)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
           <a style="color:Blue;" href="@Url.Action("edit", "account", new { id = item.id })">Edit</a>
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            <a style="color:Blue;" href="@Url.Action("delete", "account", new { id = item.id })">Delete</a>          
        </td>
        
        
       
    </tr>
}

</table>
        </td>
    </tr>
</table>
</div>