@model IEnumerable<SingSpaze.Models.songdata>

@{
    ViewBag.Title = "Song Lists";
}
<div style="padding-top: 25px;padding-bottom: 30px;">
<table style="width:100%;border: 1px solid black;text-align: center;">
    <tr style="border: 1px solid black;background-color: rgb(218, 216, 216);">
        <td style="text-align: left;padding-left: 15px;"><h2>Song Lists</h2></td>
        <td style="text-align: right;" ><input style="margin-top: 30px;" type="button" value="Create New" onclick="location.href='@Url.Action("create", "song")'" /></td>
    </tr>
    <tr>
        <td colspan="2" style="padding: 15px;">
        <table style="border-bottom:1px solid black;">    
    <tr>
        @*<th style="border: 1px solid black;text-align: center;">
            @Html.DisplayNameFor(model => model.originName)
        </th>*@
        <th style="border: 1px solid black;text-align: center;">
            Song Title
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Album
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Artist
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Language
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Genre
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Recordlabel
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Length
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Edit
        </th>
        <th style="border: 1px solid black;text-align: center;">
            Status
        </th>        
        @*<th>
            @Html.DisplayNameFor(model => model.song_releasedDate)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.song_thumbnail)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.song_picture)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.song_status)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.song_addedDate)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.song_filePath)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.song_price)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.song_view)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr style="text-align: left;">
        @*<td>
            @Html.DisplayFor(modelItem => item.originName)
        </td>*@
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.engName)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.albumdata.description_TH)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.artistdata.description_TH)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.languagedata.description)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.genredata.description)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.recordlabeldata.description)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @Html.DisplayFor(modelItem => item.length)
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
           @* @Html.ActionLink("Edit", "Edit", new { id = item.id })*@
           <a style="color:Blue;" href="@Url.Action("edit", "song", new { id = item.id })">Edit</a>
        </td>
        <td style="border-left: 1px solid black; border-right:1px solid black;text-align: left;padding: 10px;">
            @*@Html.DisplayFor(modelItem => item)*@
           @* @if (item.status == 1) { Response.Write("active"); } else { Response.Write("deactive"); }*@ 
            @if (item.status == 1)
              {
              <label style="color:Blue;">Active</label>
              }
            else
            {
              <label style="color:Red;">Deactive</label>
            }          
        </td>
        
        
        @*<td>
            @Html.DisplayFor(modelItem => item.song_keywords)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.song_lyrics)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.contentpartnerdata.description)
        </td>*@
        
        @*<td>
            @Html.DisplayFor(modelItem => item.song_releasedDate)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.song_thumbnail)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.song_picture)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.song_status)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.song_addedDate)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.song_filePath)
        </td>*@
       @* <td>
            @Html.DisplayFor(modelItem => item.song_price)
        </td>*@
       @* <td>
            @Html.DisplayFor(modelItem => item.song_view)
        </td>*@
        @*<td>
            @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
           @* @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.id })
        </td>*@
    </tr>
}

</table>
        </td>
    </tr>
</table>
</div>